#app4 {
  border: 1px solid blueviolet;
  width: 48vw;
  height: 50vh;
}
@keyframes change {
  0% {
    background: salmon;
  }
  25% {
    background: slateblue;
  }
  50% {
    background: rgb(11, 233, 48);
  }
  75% {
    background: rgb(233, 236, 8);
  }
}
#app4 .circle {
  width: 26vh;
  height: 26vh;
  border: 1px solid rosybrown;
  border-radius: 50%;
  margin-top: 20vh;
  margin-left: 20vh;
}
